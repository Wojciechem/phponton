{
    "name": "wojciechem/phponton",
    "description": "no-bullshit php microframework",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "wojciechem",
            "email": "wmiedzybrodzki@outlook.com"
        }
    ],
    "require": {
        "psr/http-message": "^1.0",
        "php": ">=7.4",
        "nyholm/psr7": "^1.4",
        "nyholm/psr7-server": "^1.0",
        "laminas/laminas-httphandlerrunner": "^1.4",
        "league/container": "^3.3",
        "monolog/monolog": "^2.2",
        "league/route": "^5.0",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
